<?xml version="1.0" encoding="UTF-8"?>

<?import customcontrols.CustomPasswordField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefWidth="220.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myeighthours.viewcontroller.LoginController">
    <children>
        <TextField fx:id="usernameField" maxWidth="Infinity" prefWidth="Infinity" promptText="username" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" />
        <CustomPasswordField fx:id="passwordField" maxWidth="Infinity" prefWidth="Infinity" promptText="password" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
        <ChoiceBox fx:id="choiceBox" maxWidth="Infinity" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
        <Button fx:id="startButton" maxWidth="Infinity" mnemonicParsing="false" prefWidth="Infinity" text="Start" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
