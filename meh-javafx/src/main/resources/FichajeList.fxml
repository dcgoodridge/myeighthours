<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<VBox maxHeight="400" maxWidth="300" minHeight="208.0" minWidth="254.0" prefHeight="208.0" prefWidth="267.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myeighthours.viewcontroller.FichajeListController">
    <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="b_fichajeAdd" mnemonicParsing="false" text="+" />
         </children>
          <Button fx:id="b_fichajeSynchronize" contentDisplay="CENTER" alignment="CENTER" prefHeight="25" prefWidth="25" mnemonicParsing="false">
              <graphic>
                  <FontAwesomeIconView glyphName="HISTORY" size="14px" glyphStyle="-fx-fill: #555555" />
              </graphic>
          </Button>
      </HBox>
        <ListView fx:id="listView" prefHeight="256.0" prefWidth="300.0" VBox.vgrow="ALWAYS" />
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
